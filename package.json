{
  "name": "robtherobot",
  "packageManager": "yarn@4.5.3",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "start": "bin/start.sh",
    "build": "yarn workspaces foreach --all --parallel run build",
    "clean": "yarn workspaces foreach --all --parallel run clean",
    "test": "yarn workspaces foreach --all --parallel run test"
  },
  "workspaces": [
    "rob-client",
    "rob-gui",
    "rob-host",
    "rob-server",
    "rob-web"
  ],
  "dependencies": {
    "pm2": "^5.4.3",
    "typescript": "^5.7.2"
  }
}
