{
  "name": "rob-client",
  "packageManager": "yarn@4.5.3",
  "type": "commonjs",
  "dependencies": {
    "bootstrap": "^5.3.3",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rob-web": "workspace:*"
  },
  "scripts": {
    "clean": "rm -rf public/static/*",
    "type-check": "tsc --noEmit",
    "build:skip-types": "esbuild --sourcemap --bundle --outdir=public/static src/main.tsx",
    "build": "yarn type-check && yarn build:skip-types"
  },
  "devDependencies": {
    "@types/bootstrap": "^5",
    "@types/lodash": "^4.17.13",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19",
    "esbuild": "^0.24.2",
    "typescript": "^5.7.2"
  }
}
